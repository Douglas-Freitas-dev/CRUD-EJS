<%- contentFor('body') %>
<div class="jumbotron text-center">
    <h1>Lista de Produtos</h1>
    <div class="row">
        <table class="table table-hover text-center">
            <tr>
                <th><h2>Código Produto</h2></th>
                <th><h2>Descrição</h2></th>
                <th><h2>Quantidade</h2></th>
                <th><h2>Preço</h2></th>
            </tr>
            <%for (p of produtos) {%>
            <tr>
                <td><h3><%=p.codigo%></h3></td>
                <td><h3><%=p.descricao%></h3></td>
                <td><h3><%=p.quantidade%></h3></td>
                <td><h3><%=p.preco%></h3></td>
            </tr>
            <%}%>
        </table>
    </div>
</div>